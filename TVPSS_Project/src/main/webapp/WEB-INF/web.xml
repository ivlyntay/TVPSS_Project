<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
	 <display-name>TVPSS Project</display-name>
	 
	 <servlet>
	 <servlet-name>myMainController</servlet-name>
	 <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
	 <load-on-startup>1</load-on-startup>
	 </servlet>
	 <servlet>
	 	<servlet-name>CrewController</servlet-name>
	 	<display-name>CrewController</display-name>
	 	<description></description>
	 	<servlet-class>com.example.controller.CrewController</servlet-class>
	 </servlet>
	 <servlet>
	 	<servlet-name>Crew_TVPSSController</servlet-name>
	 	<display-name>Crew_TVPSSController</display-name>
	 	<description></description>
	 	<servlet-class>com.example.controller.Crew_TVPSSController</servlet-class>
	 </servlet>
	 <servlet>
	 	<servlet-name>ContentController</servlet-name>
	 	<display-name>ContentController</display-name>
	 	<description></description>
	 	<servlet-class>com.example.controller.ContentController</servlet-class>
	 </servlet>
	 
	 <servlet-mapping>
	 <servlet-name>myMainController</servlet-name>
	 <url-pattern>/</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	 	<servlet-name>CrewController</servlet-name>
	 	<url-pattern>/CrewController</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	 	<servlet-name>Crew_TVPSSController</servlet-name>
	 	<url-pattern>/Crew_TVPSSController</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	 	<servlet-name>ContentController</servlet-name>
	 	<url-pattern>/ContentController</url-pattern>
	 </servlet-mapping>
</web-app> 